<div>
  <%= raw(@body) %>
  <h4>What sleep issues have you been affected with recently?</h4>
  <%= form_for @conn, homepage_path(@conn, :show), [as: :tags],  fn f -> %>
    <%= select f, :tag, @tags %>
    <%= submit "Submit" %>
  <% end %>

  <%= raw(@alphatext) %>

  <%= if assigns[:resources] do %>
    <%= render "show.html", assigns %>
  <% end %>

  <%= raw(@footer) %>
</div>
